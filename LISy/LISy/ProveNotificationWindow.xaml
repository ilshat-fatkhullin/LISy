<Window x:Name="ProveNotification" x:Class="LISy.ProveNotificationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LISy"
        mc:Ignorable="d"
        Title="ProveNotificationWindow" Height="84.668" Width="313.235" ResizeMode="NoResize" Icon="Design/icon_logo100x100.ico" WindowStartupLocation="CenterScreen">
    <Grid x:Name="MainGrid">
        <Grid.Background>
            <ImageBrush ImageSource="Design/books_shelf_back.jpg" Stretch="None"/>
        </Grid.Background>
        <CheckBox x:Name="acceptedCheckBox" Content="Accepted" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Foreground="White"/>
        <CheckBox x:Name="ignorCheckBox" Content="Ignor" HorizontalAlignment="Left" Margin="10,30,0,0" VerticalAlignment="Top" Height="16" Width="70" Foreground="White"/>
        <Button x:Name="saveButton" Content="Save" HorizontalAlignment="Left" Margin="207,10,0,10" Width="90" Click="saveButton_Click" Foreground="White" Background="{x:Null}" BorderBrush="{x:Null}"/>
    </Grid>
</Window>
